{
  "name": "react-reflex",
  "version": "1.1.5",
  "description": "Flex layout component for advanced React web applications",
  "main": "dist/es/index.js",
  "types": "./index.d.ts",
  "scripts": {
    "build-demo-dev": "webpack --watch --config ./webpack/demo/development.webpack.config",
    "build-demo": "webpack --config ./webpack/demo/production.webpack.config",
    "build-lib-dev": "webpack --config ./webpack/lib/development.webpack.config",
    "build-lib": "webpack --config ./webpack/lib/production.webpack.config",
    "build-es": "babel src/lib --presets react,es2015,stage-0 --out-dir dist/es",
    "build": "npm run build-es && npm run build-lib && npm run build-demo",
    "prepublish": "npm run build"
  },
  "author": "Philippe Leefsma",
  "license": "MIT",
  "keywords": [
    "flex",
    "layout",
    "react",
    "reactjs",
    "react-component"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/leefsmp/Re-Flex.git"
  },
  "devDependencies": {
    "babel-cli": "^6.24.1",
    "babel-core": "^6.14.0",
    "babel-eslint": "^6.0.0-beta.6",
    "babel-loader": "^6.2.5",
    "babel-plugin-istanbul": "^2.0.1",
    "babel-plugin-transform-react-constant-elements": "^6.9.1",
    "babel-plugin-transform-react-inline-elements": "^6.8.0",
    "babel-plugin-transform-react-remove-prop-types": "^0.2.10",
    "babel-plugin-transform-runtime": "^6.15.0",
    "babel-preset-es2015": "^6.14.0",
    "babel-preset-react": "^6.11.1",
    "babel-preset-stage-0": "^6.5.0",
    "babel-register": "^6.14.0",
    "babel-runtime": "^6.23.0",
    "clean-webpack-plugin": "^0.1.14",
    "css-loader": "^0.16.0",
    "es5-shim": "~4.1.10",
    "eslint": "^0.21.1",
    "eslint-plugin-react": "^2.3.0",
    "lodash.throttle": "^4.1.1",
    "node-sass": "^3.13.1",
    "prop-types": "15.5.8",
    "react": "^15.4.1",
    "react-dom": "^15.4.1",
    "react-measure": "^1.4.7",
    "sass-loader": "^2.0.1",
    "style-loader": "^0.12.4",
    "webpack": "^2.2.1"
  },
  "peerDependencies": {
    "react": "^15.4.1",
    "react-dom": "^15.4.1"
  }
}
